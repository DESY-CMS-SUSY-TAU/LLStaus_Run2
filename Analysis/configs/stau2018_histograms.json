    // General score of leading and subleading jet:

    "jet_score_TH2": {
        "bins": [
            {
                "name": "leading_jet_tag",
                "label": "P(jet_lead)",
                "n_or_arr": 140,
                "lo": -0.2,
                "hi": 1.2,
                "unit": "prob."
            },
            {
                "name": "subleading_jet_tag",
                "label": "P(jet_sublead)",
                "n_or_arr": 140,
                "lo": -0.2,
                "hi": 1.2,
                "unit": "prob."
            }
        ],
        "fill": {
            "leading_jet_tag": [
                "jet_1",
